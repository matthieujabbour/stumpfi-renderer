{
  "compilerOptions":{
    "allowJs": false,
    "alwaysStrict": true,
    "jsx": "react",
    "newLine": "LF",
    "strict": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["dom", "es7", "scripthost", "dom.iterable"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}